  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.28",
    "nodemon": "^2.0.4"
  }
}
